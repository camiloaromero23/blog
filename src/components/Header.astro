---
import { Routes } from '../enums';
import Logo from './Logo.astro';
import NavBar from './NavBar/NavBar.astro';

interface Props {
  current: Routes;
}
const { current = Routes.HOME } = Astro.props;
---

<header class="flex items-center py-4 sm:items-stretch">
  <Logo />
  <NavBar current={current} />
</header>
